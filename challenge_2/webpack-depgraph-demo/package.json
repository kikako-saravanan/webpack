{
  "name": "webpack-depgraph-demo",
  "version": "1.0.0",
  "description": "Demo: dependency graph visualization with Webpack + React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=development",
    "stats": "webpack --config webpack.config.js --mode=development --profile --json > stats.json",
    "gen-dot": "node gen-dot.js",
    "render-dot": "dot -Tpng stats-deps.dot -o stats-deps.png || echo 'Install Graphviz to render dot file.'",
    "clean": "rm -rf dist stats.json stats-deps.dot stats-deps.png"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.2",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
